{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["airbnb-base", "plugin:prettier/recommended"],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"overrides": [
		{
			"files": ["**/*.test.js"],
			"env": {
				"jest": true
			}
		}
	],
	"rules": {
		"no-console": "off"
	}
}
